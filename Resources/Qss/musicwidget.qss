QPushButton#MusicLibPushButton,
QPushButton#VideoPushButton,
QPushButton#RadioPushButton,
QPushButton#LocalMusicPushButton,
QPushButton#MyfavoritePushButton,
QPushButton#DownloadMusicPushButton,
QPushButton#HistoryPushButton,
QPushButton#PurchasePushButton
{
    border-radius:2px;
    text-align: left;
}

QPushButton#MusicLibPushButton:hover,
QPushButton#VideoPushButton:hover,
QPushButton#RadioPushButton:hover,
QPushButton#LocalMusicPushButton:hover,
QPushButton#MyfavoritePushButton:hover,
QPushButton#DownloadMusicPushButton:hover,
QPushButton#HistoryPushButton:hover,
QPushButton#PurchasePushButton:hover
{
    background-color:#d9d8d9;
}

QPushButton#MusicLibPushButton:checked,
QPushButton#VideoPushButton:checked,
QPushButton#RadioPushButton:checked,
QPushButton#LocalMusicPushButton:checked,
QPushButton#MyfavoritePushButton:checked,
QPushButton#DownloadMusicPushButton:checked,
QPushButton#HistoryPushButton:checked,
QPushButton#PurchasePushButton:checked
{
    background-color:qlineargradient(spread:pad,  x1:0, x2:1, y1:0, y2:0,
        stop: 0 #1dd6bf,
        stop: 0.25 #24d0ae,
        stop: 0.50 #28cc9e,
        stop: 0.75 #2ac890,
        stop: 1 #32c37d);
    color: white;
}

