QSlider#playSlider::groove:horizontal {
    background: #F0F0F0;
    height: 1px;
}

QSlider#playSlider::sub-page:horizontal {
    background: #2eb372
}

QSlider#playSlider::add-page:horizontal {
    background: #d9d8d9;
}

QSlider#playSlider::handle:horizontal
{
    background: #2eb372;
}

QSlider#playSlider::handle:horizontal:hover {
    width:6px;
    background-color:#2eb372;
    margin:-3px 0px -3px 0px;
    border-radius: 3px;
}

QSlider#playSlider::sub-page:horizontal:disabled {
    background: #d9d8d9;
}

QSlider#playSlider::add-page:horizontal:disabled {
    background: #d9d8d9;
}

QSlider#playSlider::handle:horizontal:disabled {
    width:6px;
    background-color:#2eb372;
    margin:-3px 0px -3px 0px;
    border-radius: 3px;
}

